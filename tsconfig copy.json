{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "ESNext",

    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/config/*": ["src/config/*"],
      "@/api/*": ["src/api/*"],
      "@/cms/*": ["src/cms/*"],
      "@/lib/*": ["src/lib/*"],
      "@/helpers/*": ["src/helpers/*"],
      "@/layouts/*": ["src/layouts/*"]
    }
  }
}
