---
import Post from '@/templates/blog/post/Post.astro';

import getPostsPaths from '@/lib/getPostsPaths';

export async function getStaticPaths() {
  const data = Astro.fetchContent('../../../content/blog/posts/*/*.??.md');
  return getPostsPaths(data, true);
}
---

<Post post={Astro.props as Post} />
